{
  "name": "chiphi-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "start:production": "node scripts/production-start.js",
    "validate:production": "node deployment/production.config.js",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:integration": "vitest run tests/**/*.integration.test.ts",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:mcp": "playwright test tests/mcp/",
    "test:mcp:comprehensive": "playwright test tests/mcp/comprehensive-mcp-integration.spec.ts",
    "test:mcp:supabase": "playwright test tests/mcp/supabase-mcp-integration.spec.ts",
    "test:mcp:playwright": "playwright test tests/mcp/playwright-mcp-integration.test.ts",
    "test:workflow": "playwright test tests/e2e/comprehensive-email-workflow.spec.ts",
    "test:workflow:advanced": "playwright test tests/e2e/advanced-email-processing-workflow.spec.ts",
    "test:isolation": "playwright test tests/e2e/multi-tenant-isolation.spec.ts",
    "test:accuracy": "playwright test tests/e2e/ai-categorization-accuracy.spec.ts",
    "test:insights": "playwright test tests/e2e/insights.spec.ts",
    "test:realtime": "playwright test tests/e2e/dashboard-realtime-updates.spec.ts",
    "test:performance": "playwright test --grep=\"performance|load|concurrent\"",
    "test:security": "playwright test --grep=\"security|pii|isolation|rls\"",
    "test:multilang": "playwright test --grep=\"spanish|french|japanese|german|translation\"",
    "test:smoke": "playwright test --grep=\"smoke\"",
    "test:regression": "npm run test:unit && npm run test:e2e --grep=\"regression\"",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:full": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:mcp",
    "test:comprehensive": "npm run test:full && npm run test:performance && npm run test:security",
    "test:ci": "npm run test:unit && npm run test:e2e -- --reporter=junit",
    "test:quick": "vitest run --reporter=basic && playwright test --grep=\"smoke|basic\"",
    "test:watch": "concurrently \"npm run test:unit:watch\" \"playwright test --ui\"",
    "test:setup": "node scripts/setup-test-environment.js",
    "test:setup:playwright": "playwright install && node tests/global-setup.ts",
    "test:cleanup": "node scripts/cleanup-test-data.js",
    "test:validate": "node scripts/validate-test-data.js",
    "test:report": "playwright show-report",
    "test:pdf": "playwright test --grep=\"pdf|attachment\"",
    "test:forwarded": "playwright test --grep=\"forwarded|nested\"",
    "test:malicious": "playwright test --grep=\"malicious|xss|suspicious\"",
    "test:duplicate": "playwright test --grep=\"duplicate|prevention\"",
    "test:edge-cases": "playwright test --grep=\"edge|malformed|timeout\"",
    "test:load": "playwright test --grep=\"load|concurrent|batch\"",
    "test:translation": "playwright test --grep=\"translation|multilingual\"",
    "test:confidence": "playwright test --grep=\"confidence|accuracy|extraction\"",
    "test:learning": "playwright test --grep=\"merchant|learning|correction\"",
    "test:dashboard": "playwright test --grep=\"dashboard|analytics|realtime\"",
    "test:export": "playwright test --grep=\"export|csv|ynab\"",
    "test:accessibility": "playwright test --grep=\"accessibility|a11y|aria\"",
    "test:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:cross-browser": "playwright test --project=chromium --project=firefox --project=webkit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.1",
    "@vercel/analytics": "latest",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "crypto-js": "^4.2.0",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.5.1",
    "geist": "latest",
    "input-otp": "1.4.1",
    "lucide-react": "^0.454.0",
    "mailparser": "^3.7.4",
    "next": "15.2.4",
    "next-themes": "^0.4.6",
    "openai": "^5.16.0",
    "react": "^19",
    "react-day-picker": "9.8.0",
    "react-dom": "^19",
    "react-hook-form": "^7.60.0",
    "react-is": "^19.1.1",
    "react-resizable-panels": "^2.1.7",
    "recharts": "latest",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.9",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/crypto-js": "^4.2.2",
    "@types/mailparser": "^3.4.6",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.1.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5",
    "tw-animate-css": "1.3.3",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
